    </style>
    <style name="RedLabel" parent="BrokerClientLabel">
        <item name="android:textColor">#f21f28</item>
    </style>
    <style name="BrokerClientField">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:textSize">15sp</item>
        <item name="android:paddingLeft">0sp</item>
        <item name="android:paddingRight">30sp</item>
        <item name="android:paddingTop">5sp</item>
        <item name="android:paddingBottom">5sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#555555</item>
    </style>
    <style name="RedField" parent="BrokerClientField">
        <item name="android:textColor">#f21f28</item>
    </style>
    <style name="Separator">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_marginLeft">0dp</item>
        <item name="android:layout_marginRight">0dp</item>
        <item name="android:layout_marginTop">15dp</item>
        <item name="android:layout_marginBottom">15dp</item>
        <item name="android:background">#e1e1e1</item>
    </style>
    <style name="NormalSeparator">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:layout_marginLeft">0dp</item>
        <item name="android:layout_marginRight">0dp</item>
        <item name="android:layout_marginTop">8sp</item>
        <item name="android:layout_marginBottom">8sp</item>
        <item name="android:background">#e1e1e1</item>
    </style>

    <style name="IconButton">
        <item name="android:layout_width">50sp</item>
        <item name="android:layout_height">50sp</item>
        <item name="android:layout_margin">12sp</item>
        <item name="android:padding">0sp</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:background">@android:color/transparent</item>
    </style>
    <style name="Title">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxLines">2</item>
        <item name="android:paddingTop">20dp</item>
        <item name="android:paddingBottom">20dp</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#555555</item>
        <item name="android:textSize">25dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:ellipsize">end</item>
    </style>
    <style name="Subtitle">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#daa520</item>
        <item name="android:textSize">18dp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="Title2">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxLines">2</item>
        <item name="android:layout_margin">0sp</item>
        <item name="android:padding">0sp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/textgray</item>
        <item name="android:textSize">25sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:ellipsize">end</item>
    </style>
    <style name="Subtitle2">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_margin">0sp</item>
        <item name="android:padding">0sp</item>
        <item name="android:textColor">@color/textgray</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="enrollment_status_subtitle" parent="Subtitle2">
