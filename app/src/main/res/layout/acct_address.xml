<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent"
                                             android:background="@color/white"
    >

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="What is your\nhome address?"
        android:textAlignment="center"
        android:textSize="32dp"
        android:textStyle="bold"
        android:background="@color/transparent"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="45dp"
        android:layout_marginLeft="45dp"
        android:layout_marginRight="45dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <EditText
        android:id="@+id/address1"
        android:inputType="text"
        android:hint="Address"
        android:textSize="24dp"
        android:background="@color/transparent"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/textView9"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="45dp"
        android:layout_marginRight="45dp"
        android:paddingTop="10dp"
        android:paddingBottom="9dp"
        />
    <View
        android:id="@+id/addressUnderline"
        android:layout_width="0dp"
        android:layout_height="1dp"
        style="@style/NormalSeparator"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/address1"
        android:background="@color/mainblue"
        android:layout_marginLeft="45dp"
        android:layout_marginRight="45dp"
        />
    <EditText
        android:id="@+id/address2"
        android:inputType="textPersonName"
        android:hint="Address 2"
        android:textSize="24dp"
        android:background="@color/transparent"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/addressUnderline"
        android:layout_marginLeft="45dp"
        android:layout_marginRight="45dp"
        android:layout_marginTop="25dp"
        android:paddingTop="10dp"
        android:paddingBottom="9dp"
        />
    <View
        android:id="@+id/address2Underline"
        android:layout_width="0dp"
        android:layout_height="1dp"
        style="@style/NormalSeparator"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/address2"
        android:background="@color/mainblue"
        android:layout_marginLeft="45dp"
        android:layout_marginRight="45dp"
        />

    <EditText
        android:id="@+id/city"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:inputType="textEmailAddress"
        android:textSize="24dp"
        android:background="@color/transparent"
        android:hint="City"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/address2Underline"
        android:layout_marginLeft="45dp"
        android:layout_marginRight="45dp"
        android:layout_marginTop="25dp"
        android:paddingTop="10dp"
        android:paddingBottom="9dp"
        />
    <View
        android:id="@+id/cityUnderline"
        android:layout_width="0dp"
        android:layout_height="1dp"
        style="@style/NormalSeparator"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/city"
        android:background="@color/mainblue"
        android:layout_marginLeft="45dp"
        android:layout_marginRight="45dp"
        />

    <EditText
        android:id="@+id/state"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:hint="State"
        android:inputType="textPassword"
        android:textSize="24dp"
        android:background="@color/transparent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cityUnderline"
        android:layout_marginLeft="45dp"
        android:layout_marginRight="45dp"
        android:layout_marginTop="25dp"
        android:paddingTop="10dp"
        android:paddingBottom="9dp"
        />
    <View
        android:id="@+id/stateUnderline"
        android:layout_width="0dp"
        android:layout_height="1dp"
        style="@style/NormalSeparator"
        app:layout_constraintLeft_toLeftOf="@+id/state"
        app:layout_constraintRight_toRightOf="@+id/state"
        app:layout_constraintTop_toBottomOf="@+id/state"
        android:background="@color/mainblue"
        />

    <EditText
        android:id="@+id/zipCode"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:hint="ZIP Code"
        android:inputType="textPassword"
        android:textSize="24dp"
        android:background="@color/transparent"
        app:layout_constraintLeft_toRightOf="@+id/state"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cityUnderline"
        android:layout_marginLeft="45dp"
        android:layout_marginRight="45dp"
        android:layout_marginTop="25dp"
        android:paddingTop="10dp"
        android:paddingBottom="9dp"
        />
    <View
        android:id="@+id/zipCodeUnderline"
        android:layout_width="0dp"
        android:layout_height="1dp"
        style="@style/NormalSeparator"
        app:layout_constraintLeft_toLeftOf="@+id/zipCode"
        app:layout_constraintRight_toRightOf="@+id/zipCode"
        app:layout_constraintTop_toBottomOf="@+id/zipCode"
        android:background="@color/mainblue"
        />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/transparent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginRight="45dp"
        android:layout_marginBottom="35dp"
        app:srcCompat="@drawable/right_arrow_vector"
        />

</android.support.constraint.ConstraintLayout>